# Configuración del Frontend para Producción
# Dominio: getdevtools.com

# URL de la API
VITE_API_URL=https://getdevtools.com/api

# Configuración de la aplicación
VITE_APP_NAME=WEEKLY
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Dominio principal
VITE_DOMAIN=getdevtools.com

# Configuración de tenants
VITE_DEFAULT_TENANT=demo
VITE_ADMIN_SUBDOMAIN=admin

# Configuración de características
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_NOTIFICATIONS=true

# Configuración de desarrollo (desactivar en producción)
VITE_DEV_MODE=false
